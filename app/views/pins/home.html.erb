<% if @current_user.nil? %>
  <div class="welcome">
      <h1>Welcome</h1>

      <%= link_to "Sign Up", sign_up_path, remote: true, class: "btn btn-default"  %>
       or <%= link_to "Sign In", sign_in_path, remote: true, class: "btn btn-default" %>
  </div>
<% else %>
  <%= render partial: 'wall' %>
<% end %>
